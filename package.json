    "concurrently": "^5.3.0",
    "core-js": "^2.6.5",
    "dat.gui": "^0.7.7",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "three": "^0.122.0",
    "vue": "^2.6.10",
    "vue-cli-plugin-tailwind": "~1.5.0",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "vue-cli-plugin-tailwind/purgecss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
