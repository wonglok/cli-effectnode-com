    "minimist": "^1.2.5",
    "open": "^7.3.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "git add .; git commit -am \"git backup\"; git push -u origin master",
    "dev": "concurrently \"npm run d3002\" \"npm run ssl\"",
    "d3002": "vue-cli-service serve --port 3002",
    "ssl": "local-ssl-proxy --config proxy.config.json"
  },
  "devDependencies": {
    "@tweenjs/tween.js": "tweenjs/tween.js",
